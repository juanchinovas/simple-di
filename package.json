{
  "name": "@sunacchi/simple-di",
  "version": "1.1.0",
  "description": "Simple dependency injection for simple project",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "repository": {
	  "type": "git",
	  "directory": "./",
	  "url": "https://github.com/juanchinovas/simple-di.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run build && jest ./lib/**/*.spec.js",
    "testit": "npm run build && jest",
    "test:coverage": "npm run build && jest ./lib/**/*.spec.js --coverage"
  },
  "keywords": [
    "di",
    "dependency injection",
    "dependency injector",
    "injector",
	"javascript",
	"simple",
	"simple-di"
  ],
  "author": "juanchinovas",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1"
  },
  "publishConfig": {
	  "access": "public"
  },
  "files": [
	  "/lib/common.js",
	  "/lib/container.js",
	  "/lib/decorators.js",
	  "/lib/index.js",
	  "/types/container.d.ts",
	  "/types/decorators.d.ts",
	  "/types/index.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  }
}
