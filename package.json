{
  "name": "@sunacchi/simple-di",
  "version": "1.2.2",
  "description": "Simple dependency injection for simple project",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "directory": "./",
    "url": "https://github.com/juanchinovas/simple-di.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run build && jest ./test/*.spec.ts",
    "test:coverage": "npm run build && jest ./test/*.spec.ts --coverage",
    "publish:pack": "npm run build && npm run test:coverage && npm publish"
  },
  "keywords": [
    "di",
    "dependency injection",
    "dependency injector",
    "injector",
    "javascript",
    "simple",
    "simple-di",
    "js"
  ],
  "author": "juanchinovas",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "jest": "^27.5.1",
    "ts-jest": "27.1",
    "typescript": "^4.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/lib/common.js",
    "/lib/container.js",
    "/lib/decorators.js",
    "/lib/index.js",
    "/types/container.d.ts",
    "/types/decorators.d.ts",
    "/types/common.d.ts",
    "/types/index.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  }
}
